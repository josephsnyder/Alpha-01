run ${CControl} cterminal ${SERVER} XINDEX ${INSTANCE}
WinWait, Cache
;WinHide Cache
send !l
WinWaitNotActive
send ${VISTA_BINARY_DIR}/Testing/Temporary/${Letter}Test`r
send `r
